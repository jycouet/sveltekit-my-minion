{
  "name": "@sveltekit-my-minion/graphql-codegen-stores",
  "version": "0.0.2",
  "description": "GraphQL Code Generator plugin to generate SvelteKit stores in My Minion context",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/sveltekit-my-minion.git",
    "directory": "packages/graphql-codegen-stores"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman --config ../../jest.config.js",
    "prepack": "bob prepack"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "2.1.0",
    "@graphql-codegen/visitor-plugin-common": "2.1.0",
    "auto-bind": "4.0.0",
    "change-case-all": "1.0.14",
    "tslib": "2.3.0"
  },
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0",
    "graphql-tag": "^2.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
